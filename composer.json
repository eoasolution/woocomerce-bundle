    "require": {
        "php": ">=7.0",
        "automattic/woocommerce": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nipe Setiawan Harefa",
            "email": "nipeharefa@gmail.com"
